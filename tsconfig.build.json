{
  "compilerOptions": {
    "module": "esnext",
    "lib": ["es5", "es6", "ES2019", "dom", "dom.iterable"],
    "target": "ES5",
    "importHelpers": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
  }
}
